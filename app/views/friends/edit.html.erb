<h1>Editing Friend</h1>
<br/>
<%= render 'form', friend: @friend %>
<br/>
<table>
<th><%= link_to 'Show', @friend, class: 'btn btn-secondary' %></th>
<th><%= button_to 'Delete', @friend, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %></th>
<th><%= link_to 'Back', friends_path, class: 'btn btn-dark' %></th>
</table>
